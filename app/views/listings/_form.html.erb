<%= render "layouts/errors", :object => @listing %>

<%= bootstrap_form_for @listing do |f| %>
<div class="form-group">
  <%= f.text_field :title %>
</div>
<div class="form-group">
  <%= f.text_field :description %>
</div>
<div class="form-group">
  <%= f.text_field :location %>
</div>
<div class="form-group">
  <%= f.file_field :avatar, {label: "Image"} %>
</div>
<%= f.submit class: "btn btn-success" %>
<% end %>
