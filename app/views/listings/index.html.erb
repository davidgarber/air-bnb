<div class="col-md-5">
  <h1>Listings</h1>
  <hr>
  <ul class="list-unstyled">
    <% @listings.each do |listing| %>
    <li class="well">
      <h3><%= link_to listing.title, listing_path(listing) %></h3>
      <p><%= link_to listing do %>
        <%= image_tag listing.avatar.url(:medium) %></p>
    </li>
  </ul>
    <% end %>
  <% end %>
  <hr>
  <br>

  <%= link_to "Add a new listing", new_listing_path, :class => "btn btn-success btn-sm" %><br>
</div>
